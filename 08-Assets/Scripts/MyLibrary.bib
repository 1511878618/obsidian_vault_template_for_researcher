@article{Mateu-Regue2021,
  title = {Unveiling {{mRNP}} Composition by Fluorescence Correlation and Cross-Correlation Spectroscopy Using Cell Lysates},
  author = {Mateu-Regué, Àngels and Christiansen, Jan and Bagger, Frederik Otzen and Hellriegel, Christian and Nielsen, Finn Cilius},
