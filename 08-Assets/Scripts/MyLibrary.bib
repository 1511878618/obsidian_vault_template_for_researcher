@article{SoundaraRajan2020,
  title = {Tunneling {{Nanotubes-Mediated Protection}} of {{Mesenchymal Stem Cells}}: {{An Update}} from {{Preclinical Studies}}},
  shorttitle = {Tunneling {{Nanotubes-Mediated Protection}} of {{Mesenchymal Stem Cells}}},
  author = {Soundara Rajan, Thangavelu and Gugliandolo, Agnese and Bramanti, Placido and Mazzon, Emanuela},
  date = {2020-05-14},
  journaltitle = {International Journal of Molecular Sciences},
