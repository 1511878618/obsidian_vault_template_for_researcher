  author = {Segel, Michael and Lash, Blake and Song, Jingwei and Ladha, Alim and Liu, Catherine C. and Jin, Xin and Mekhedov, Sergei L. and Macrae, Rhiannon K. and Koonin, Eugene V. and Zhang, Feng},
