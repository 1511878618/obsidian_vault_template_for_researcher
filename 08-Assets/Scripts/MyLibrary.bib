@article{Blanchard2021,
  title = {Turn-Key Mapping of Cell Receptor Force Orientation and Magnitude Using a Commercial Structured Illumination Microscope},
  author = {Blanchard, Aaron and Combs, J. Dale and Brockman, Joshua M. and Kellner, Anna V. and Glazier, Roxanne and Su, Hanquan and Bender, Rachel L. and Bazrafshan, Alisina S. and Chen, Wenchun and Quach, M. Edward and Li, Renhao and Mattheyses, Alexa L. and Salaita, Khalid},
  date = {2021-12},
