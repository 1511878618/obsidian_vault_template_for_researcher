@article{Mao2016,
  title = {Activity Modulation and Allosteric Control of a Scaffolded {{DNAzyme}} Using a Dynamic {{DNA}} Nanostructure},
  author = {Mao, Xiuhai and Simon, Anna J. and Pei, Hao and Shi, Jiye and Li, Jiang and Huang, Qing and Plaxco, Kevin W. and Fan, Chunhai},
  date = {2016},
  journaltitle = {Chemical Science},
