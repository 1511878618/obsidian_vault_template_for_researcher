@article{Schaffter2022,
  title = {Cotranscriptionally Encoded {{RNA}} Strand Displacement Circuits},
  author = {Schaffter, Samuel W. and Strychalski, Elizabeth A.},
  date = {2022-03-25},
  journaltitle = {Science Advances},
  shortjournal = {Sci. Adv.},
