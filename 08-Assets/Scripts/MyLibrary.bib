@article{Lee2019,
  title = {{{pH-Responsive}} i-Motif {{Conjugated Hyaluronic Acid}}/{{Polyethylenimine Complexes}} for {{Drug Delivery Systems}}},
  author = {Lee, Gyeong Jin and Kim, Tae-il},
  date = {2019-05-27},
  journaltitle = {Pharmaceutics},
