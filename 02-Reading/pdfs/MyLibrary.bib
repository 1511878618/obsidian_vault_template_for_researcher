@article{Mustafa2021,
  title = {Interrogating Accessibility of Telomeric Sequences with {{FRET}}-{{PAINT}}: Evidence for Length-Dependent Telomere Compaction},
  shorttitle = {Interrogating Accessibility of Telomeric Sequences with {{FRET}}-{{PAINT}}},
  author = {Mustafa, Golam and Shiekh, Sajad and Gc, Keshav and Abeysirigunawardena, Sanjaya and Balci, Hamza},
  date = {2021-04-06},
  journaltitle = {Nucleic Acids Research},
  volume = {49},
  number = {6},
  pages = {3371--3380},
  issn = {0305-1048, 1362-4962},
  doi = {10.1093/nar/gkab067},
  url = {https://academic.oup.com/nar/article/49/6/3371/6166857},
  urldate = {2021-10-18},
  abstract = {Abstract             Single-stranded telomeric overhangs are ∼200 nucleotides long and can form tandem G-quadruplex (GQ) structures, which reduce their accessibility to nucleases and proteins that activate DNA damage response. Whether these tandem GQs further stack to form compact superstructures, which may provide better protection for longer telomeres, is not known. We report single-molecule measurements where the accessibility of 24–144 nucleotide long human telomeric DNA molecules is interrogated by a short PNA molecule that is complementary to a single GGGTTA repeat, as implemented in the FRET-PAINT method. Binding of the PNA strand to available GGGTTA sequences results in discrete FRET bursts which were analyzed in terms of their dwell times, binding frequencies, and topographic distributions. The binding frequencies were greater for binding to intermediate regions of telomeric DNA compared to 3′- or 5′-ends, suggesting these regions are more accessible. Significantly, the binding frequency per telomeric repeat monotonically decreased with increasing telomere length. These results are consistent with telomeres forming more compact structures at longer lengths, reducing accessibility of these critical genomic sites.},
  langid = {english},
  file = {@Mustafa2021.md;@Mustafa2021.md;2021-Interrogating accessibility of telomeric sequences with FRET-PAINT.pdf}
}

