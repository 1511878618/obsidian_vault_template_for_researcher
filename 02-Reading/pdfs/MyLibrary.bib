@article{Chen2018a,
  title = {Precise Nanomedicine for Intelligent Therapy of Cancer},
  author = {Chen, Huabing and Gu, Zhanjun and An, Hongwei and Chen, Chunying and Chen, Jie and Cui, Ran and Chen, Siqin and Chen, Weihai and Chen, Xuesi and Chen, Xiaoyuan and Chen, Zhuo and Ding, Baoquan and Dong, Qian and Fan, Qin and Fu, Ting and Hou, Dayong and Jiang, Qiao and Ke, Hengte and Jiang, Xiqun and Liu, Gang and Li, Suping and Li, Tianyu and Liu, Zhuang and Nie, Guangjun and Ovais, Muhammad and Pang, Daiwen and Qiu, Nasha and Shen, Youqing and Tian, Huayu and Wang, Chao and Wang, Hao and Wang, Ziqi and Xu, Huaping and Xu, Jiang-Fei and Yang, Xiangliang and Zhu, Shuang and Zheng, Xianchuang and Zhang, Xianzheng and Zhao, Yanbing and Tan, Weihong and Zhang, Xi and Zhao, Yuliang},
