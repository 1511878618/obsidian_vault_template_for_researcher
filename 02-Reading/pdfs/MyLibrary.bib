  author = {Wickham, Shelley F. J. and Auer, Alexander and Min, Jianghong and Ponnuswamy, Nandhini and Woehrstein, Johannes B. and Schueder, Florian and Strauss, Maximilian T. and Schnitzbauer, Jörg and Nathwani, Bhavik and Zhao, Zhao and Perrault, Steven D. and Hahn, Jaeseung and Lee, Seungwoo and Bastings, Maartje M. and Helmig, Sarah W. and Kodal, Anne Louise and Yin, Peng and Jungmann, Ralf and Shih, William M.},
  date = {2020-12},
  journaltitle = {Nature Communications},
  shortjournal = {Nat Commun},
  volume = {11},
  number = {1},
  pages = {5768},
  issn = {2041-1723},
  doi = {10.1038/s41467-020-18910-x},
  url = {http://www.nature.com/articles/s41467-020-18910-x},
  urldate = {2021-10-18},
  abstract = {Abstract             DNA origami, in which a long scaffold strand is assembled with a many short staple strands into parallel arrays of double helices, has proven a powerful method for custom nanofabrication. However, currently the design and optimization of custom 3D DNA-origami shapes is a barrier to rapid application to new areas. Here we introduce a modular barrel architecture, and demonstrate hierarchical assembly of a 100 megadalton DNA-origami barrel of \textasciitilde 90\,nm diameter and \textasciitilde 250\,nm height, that provides a rhombic-lattice canvas of a thousand pixels each, with pitch of \textasciitilde 8\,nm, on its inner and outer surfaces. Complex patterns rendered on these surfaces were resolved using up to twelve rounds of Exchange-PAINT super-resolution microscopy. We envision these structures as versatile nanoscale pegboards for applications requiring complex 3D arrangements of matter, which will serve to promote rapid uptake of this technology in diverse fields beyond specialist groups working in DNA nanotechnology.},
  langid = {english},
  file = {@Wickham2020.md;2020-Complex multicomponent patterns rendered on a 3D DNA-barrel pegboard.pdf}
}

